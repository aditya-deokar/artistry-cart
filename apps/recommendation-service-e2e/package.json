{
  "name": "@artistry-cart/recommendation-service-e2e",
  "version": "0.0.1",
  "private": true,
  "nx": {
    "implicitDependencies": [
      "@artistry-cart/recommendation-service"
    ],
    "targets": {
      "e2e": {
        "executor": "nx:run-commands",
        "options": {
          "command": "npx vitest run --config apps/recommendation-service-e2e/vitest.config.ts"
        },
        "dependsOn": [
          "@artistry-cart/recommendation-service:build",
          "@artistry-cart/recommendation-service:serve"
        ]
      }
    }
  }
}
