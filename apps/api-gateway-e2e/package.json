{
  "name": "@artistry-cart/api-gateway-e2e",
  "version": "0.0.1",
  "private": true,
  "nx": {
    "implicitDependencies": [
      "@artistry-cart/api-gateway"
    ],
    "targets": {
      "e2e": {
        "executor": "nx:run-commands",
        "options": {
          "command": "npx vitest run --config apps/api-gateway-e2e/vitest.config.ts"
        },
        "dependsOn": [
          "@artistry-cart/api-gateway:build",
          "@artistry-cart/api-gateway:serve"
        ]
      }
    }
  }
}
